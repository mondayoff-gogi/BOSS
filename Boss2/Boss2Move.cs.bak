        }
        for(int i = 1; i < foothold_position.Length - 1; i++)
        {
            int temp = Random.Range(-1, 2);
            bombs[i].GetComponent<Boss2_SameDir>().Move(new Vector3(temp, 0, 0));            
        }
        if (tmp == 0)
